class MyClass(GeneratedClass):

    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        #put initialization code here
        self.almemory = ALProxy("ALMemory")
        self.motion = ALProxy("ALMotion")
        go = False
        self.tab = [False]*2
        self.murRefDist = None


    def onUnload(self):
        #put clean-up code here
        go = False
        #pass

    def onInput_onStart(self, p):
        self.onStart(0,p)

    def onInput_SuiviDeMur(self, p):
        self.onStart(1,p)

    def onStart(self, nInput,p):
        self.tab[nInput]=p
        dx = 0
        dy = 0
        dtheta = 0

        #Si on reçoit des informations
        if (self.tab[0] != False and self.tab[1] != False) :
            #Stockage des infos
            commande=self.tab[0]
            suiviDeMur=self.tab[1]
            self.log(suiviDeMur)

            #correction de la commande
            targetDistance = (commande-0.2191)/0.9893

            #Initialisation
            self.motion.setAngles("Head", [0, 0], 0.1)
            DistanceDuMur = self.getParameter("DistanceDuMur")
            DeplacementCentreParam = self.getParameter("DeplacementCentre")
            self.murRefDist = self.suiviOuPas(suiviDeMur, DeplacementCentreParam)
            useSensorValues = True
            originalPosition = self.motion.getRobotPosition(useSensorValues)

            #Top Départ
            go = True
            while go:
                dx = self.isRunning(DeplacementCentreParam, dx, dy, dtheta, suiviDeMur, DistanceDuMur)
                #Évaluation de la position actuelle
                currentPosition = self.motion.getRobotPosition(useSensorValues)
                deplacement = [currentPosition[0]-originalPosition[0],currentPosition[1]-originalPosition[1]]
                distanceParcourue = (math.sqrt(deplacement[0]**2+deplacement[1]**2))
                self.log(distanceParcourue)
                #Vérification: Est-ce que la distance voulue est atteinte?
                if targetDistance-distanceParcourue <0.01:
                    go = False
                    self.log(go)
                    self.motion.moveToward(0, 0, 0)

                time.sleep(0.1)

            self.onStopped() #activate the output of the box
            self.log("terminus")

    def isRunning(self, DeplacementCentreParam, dx , dy, dtheta, suiviDeMur, DistanceDuMur):
        """
            Calcul des paramètre puis déplacement du robot
        """

        #choix du sens de décalage
        DeplacementCentre = self.sensDecalage(DeplacementCentreParam)
        dy = self.calculDy(DeplacementCentre, DistanceDuMur)
        self.log(dy)
        dx = self.calculDx(dx, dy)
        self.log(dx)
        dtheta = self.calculDTheta(suiviDeMur, dx, dy)

        #Déplacement robot
        self.motion.moveToward(dx, dy, dtheta)

        return dx


    def calculDy(self, DeplacementCentre, DistanceDuMur):
        """
            Calcul de la valeur de dy à partir des paramètres et de la distance de Pepper par rapport aux murs
        """

        dist_gauche = self.almemory.getData("Device/SubDeviceList/Platform/LaserSensor/Left/Horizontal/Seg07/X/Sensor/Value")
        dist_droit = self.almemory.getData("Device/SubDeviceList/Platform/LaserSensor/Right/Horizontal/Seg07/X/Sensor/Value")

        #calcul
        if DeplacementCentre == True:
            return ((dist_gauche-dist_droit)/(dist_gauche+dist_droit))*0.5
        else:
            if self.murRefDist == "droit":
                if (dist_droit > (DistanceDuMur+0.1)):
                    return 0.2
                elif (dist_droit <= (DistanceDuMur-0.1)):
                    return -0.35
                else:
                    return 0
            elif self.murRefDist == "gauche":
                if (dist_gauche > (DistanceDuMur+0.1)):
                    return -0.2
                elif (dist_gauche <= (DistanceDuMur-0.1)):
                    return 0.35
                else:
                    return 0
            elif self.murRefDist == None:
                return 0



    def sensDecalage(self, DeplacementCentreParam):
        """
            Choix du sens de decalage de Pepper, pour maintenir une certaine distance de chaque mur
        """
        dist_gauche = self.almemory.getData("Device/SubDeviceList/Platform/LaserSensor/Left/Horizontal/Seg07/X/Sensor/Value")
        dist_droit = self.almemory.getData("Device/SubDeviceList/Platform/LaserSensor/Right/Horizontal/Seg07/X/Sensor/Value")
        if dist_gauche > 1.5:
            self.murRefDist = "droit"
            return False
        elif dist_gauche > 1.5:
            self.murRefDist = "gauche"
            return False
        else:
            return DeplacementCentreParam

    def calculDx(self, dx, dy):
        """
            Calcul de la valeur de dx à partir de la valeur dy
        """
        if dy < 0.15:
            dx = dx + 0.8
            if dx > 1:
                return 1
            else :
                return dx
        else :
            dx = dx - 0.8
            if dx < 0:
                return 0
            else :
                return dx

    def calculDTheta(self, suiviDeMur, dx, dy):
        """
            Calcul de dTheta à partir des capteurs et de la trigonométrie
        """
        if suiviDeMur == "droit":
            dist_a = self.almemory.getData("Device/SubDeviceList/Platform/LaserSensor/Right/Horizontal/Seg02/X/Sensor/Value")
            dist_b = self.almemory.getData("Device/SubDeviceList/Platform/LaserSensor/Right/Horizontal/Seg14/X/Sensor/Value")
            dist_c = math.sqrt(dist_a**2+dist_b**2-2*dist_a*dist_b*math.cos(math.pi/3))

            theta = float((math.pi/3) - (math.acos((dist_c**2+dist_a**2-dist_b**2)/(2*dist_c*dist_a))))

            return -(theta)

        elif suiviDeMur == "gauche":
            dist_a = self.almemory.getData("Device/SubDeviceList/Platform/LaserSensor/Left/Horizontal/Seg02/X/Sensor/Value")
            dist_b = self.almemory.getData("Device/SubDeviceList/Platform/LaserSensor/Left/Horizontal/Seg14/X/Sensor/Value")
            dist_c = math.sqrt(dist_a**2+dist_b**2-2*dist_a*dist_b*math.cos(math.pi/3))

            theta = float((math.pi/3) - (math.acos((dist_c**2+dist_a**2-dist_b**2)/(2*dist_c*dist_a))))

            return -(theta)

        else:
            dy = 0
            dx = 0
            return 0

    def suiviOuPas(self, suiviDeMur, DeplacementCentreParam):
        """
            Choix du mode suivi de mur, ou déplacement au centre du couloir
        """
        if DeplacementCentreParam == False:
            if suiviDeMur == "droit" or suiviDeMur == "gauche":
                return suiviDeMur
            else:
                return None


